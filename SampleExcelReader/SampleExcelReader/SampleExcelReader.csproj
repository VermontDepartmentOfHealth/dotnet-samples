    <TargetFramework>netcoreapp3.1</TargetFramework>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Clients.xlsx" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="Clients.xlsx">
